let num2=number("3.14");
let num3=number("3.12 is fun");

console.log(num1) //Result:42
console.log(num2) //Result:3.14
console.log(num3) //Result:NaN

let val1=number("42");
let val2=number("3.14");
let val3=number("42 is fun");

console.log(val1) //Result:42
console.log(val2) //Result:3.14
console.log(val3) //Result:42
</script>
